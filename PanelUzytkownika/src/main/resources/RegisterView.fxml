      <TextField fx:id="userName" layoutX="56.0" layoutY="110.0" promptText="Username" />
      <TextField fx:id="email" layoutX="56.0" layoutY="140.0" promptText="Email" />
      <PasswordField fx:id="password" layoutX="56.0" layoutY="170.0" promptText="Password" />
      <PasswordField fx:id="repeatPassword" layoutX="56.0" layoutY="200.0" promptText="Repeat Password" />
      <Button layoutX="96.0" layoutY="232.0" mnemonicParsing="false" onAction="#registerUser" prefHeight="25.0" prefWidth="69.0" text="Register" />
