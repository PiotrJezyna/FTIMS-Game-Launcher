<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="root" prefHeight="340.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fgl.userPanel.RegisterViewControl">
   <children>
      <TextField fx:id="name" layoutX="50.0" layoutY="50.0" promptText="Name" />
      <TextField fx:id="surname" layoutX="50.0" layoutY="80.0" promptText="Surname" />
      <TextField fx:id="userName" layoutX="50.0" layoutY="110.0" promptText="Username" />
      <TextField fx:id="email" layoutX="50.0" layoutY="140.0" promptText="Email" />
      <PasswordField fx:id="password" layoutX="50.0" layoutY="170.0" promptText="Password" />
      <PasswordField fx:id="repeatPassword" layoutX="50.0" layoutY="200.0" promptText="Repeat Password" />
      <Button layoutX="90.0" layoutY="232.0" mnemonicParsing="false" onAction="#registerUser" prefHeight="25.0" prefWidth="69.0" text="Register" />
      <Button layoutX="31.0" layoutY="265.0" mnemonicParsing="false" onAction="#openLoginScene" prefHeight="0.0" prefWidth="187.0" text="Already have an account? Login" />
   </children>
</AnchorPane>
