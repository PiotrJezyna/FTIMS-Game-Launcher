<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="340.0" prefWidth="260.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fgl.userPanel.LoginViewControl">
   <children>
      <Button layoutX="30.0" layoutY="169.0" mnemonicParsing="false" onAction="#openRegisterScene" prefHeight="0.0" prefWidth="200.0" text="Don't have an account? Register" />
      <TextField fx:id="username" layoutX="53.0" layoutY="49.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="53.0" layoutY="90.0" promptText="Password" />
      <Button layoutX="105.0" layoutY="131.0" mnemonicParsing="false" onAction="#loginButtonClicked" text="Login" />
   </children>
</AnchorPane>
